<h1>Log In</h1>

<%= form_with(url: login_path, local: true) do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.email_field :email, autofocus: true %>
  </div>
  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Log In" %>
  </div>
<% end %>

